{
  "name": "@httpjpg/ui",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./components": {
      "types": "./src/components/index.ts",
      "import": "./src/components/index.ts"
    },
    "./styles/global": {
      "types": "./src/styles/global.ts",
      "import": "./src/styles/global.ts"
    },
    "./css": {
      "types": "./styled-system/css/index.d.ts",
      "import": "./styled-system/css/index.mjs"
    },
    "./patterns": {
      "types": "./styled-system/patterns/index.d.ts",
      "import": "./styled-system/patterns/index.mjs"
    },
    "./tokens": {
      "types": "./styled-system/tokens/index.d.ts",
      "import": "./styled-system/tokens/index.mjs"
    },
    "./styled-system/*": "./styled-system/*"
  },
  "files": [
    "src",
    "styled-system",
    "panda.config.ts"
  ],
  "scripts": {
    "prepare": "panda codegen",
    "dev": "panda codegen --watch",
    "build": "panda codegen",
    "type-check": "tsc --noEmit",
    "lint": "biome lint .",
    "lint:fix": "biome lint . --fix",
    "clean": "rm -rf styled-system"
  },
  "dependencies": {
    "@httpjpg/tokens": "workspace:*"
  },
  "devDependencies": {
    "@httpjpg/tsconfig": "workspace:*",
    "@biomejs/biome": "2.3.4",
    "@pandacss/dev": "^0.45.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.0",
    "react": "^18.3.1",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "react": "^18.3.1"
  }
}
