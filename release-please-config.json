{
  "pull-request-header": ":candy: Automated Release PR",
  "packages": {
    ".": {
      "changelog-path": "CHANGELOG.md",
      "release-type": "node",
      "bump-minor-pre-major": false,
      "bump-patch-for-minor-pre-major": false,
      "include-component-in-tag": false,
      "include-v-in-tag": true,
      "draft": false,
      "prerelease": false,
      "changelog-sections": [
        { "type": "feat", "section": ":sparkles: Features" },
        { "type": "fix", "section": ":bug: Bug Fixes" },
        { "type": "perf", "section": ":racehorse: Performance Improvements" },
        { "type": "revert", "section": ":rewind: Reverts" },
        { "type": "chore", "section": ":broom: Chores" },
        { "type": "docs", "section": ":books: Documentation" },
        { "type": "style", "section": ":lipstick: Styles" },
        { "type": "refactor", "section": ":hammer: Refactors" },
        { "type": "test", "section": ":rotating_light: Tests", "hidden": true },
        { "type": "build", "section": ":construction_worker: Build System" },
        {
          "type": "ci",
          "section": ":rocket: Continuous Integration",
          "hidden": true
        }
      ]
    }
  },
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json"
}
